{
  "name": "lartisan/laravel-preset",
  "type": "preset",
  "description": "A TALL Admin Panel for Laravel.",
  "keywords": [
    "laravel preset",
    "laravel admin panel",
    "tailwind css",
    "alpine js",
    "laravel",
    "livewire"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Cristian Iosif",
      "email": "cristianiosif@me.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Lartisan\\": "src/"
    }
  },
  "require": {
    "consoletvs/charts": "^7.2",
    "laravel/ui": "^3.2",
    "livewire/livewire": "^2.4",
    "realrashid/sweet-alert": "^3.2"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lartisan\\Providers\\LartisanServiceProvider"
      ]
    }
  }
}
